# Example environment configuration for Benchkit
# If using direnv and Nix, enable the flake (requires direnv installed)
# use flake

# PostgreSQL configuration
export PGHOST=127.0.0.1
export PGPORT=5432
export PGDATABASE=benchmarks
export PGUSER=benchkit
export PGPASSWORD=benchpass  # Secure this in production

# Object Storage (e.g., AWS S3 or MinIO)
export KEY_ID=<your_id>
export SECRET_ACCESS_KEY=<your_password>  # Secure this in production

# Benchcoin logging
export RUST_LOG=info
